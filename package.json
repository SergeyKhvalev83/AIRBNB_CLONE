  "description": "Fullstack Booking App using MERN (mongo, express, react, node)",
  "engines": {
    "node": "v20.12.2"
  },
  "main": "index.js",
"scripts": {
    "client-install": "npm install --prefix client",
    "server": "node server/index.js",
